apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  annotations:
    kompose.cmd: kompose convert -f ../../docker-compose.yml
    kompose.version: 1.14.0 ()
  creationTimestamp: null
  labels:
    io.kompose.service: jobs
  name: jobs
spec:
  replicas: 1
  strategy: {}
  template:
    metadata:
      creationTimestamp: null
      labels:
        io.kompose.service: jobs
    spec:
      containers:
        - name: jobs
          image: gcr.io/integrated-farm-manager/ifm:<%= current_sha %>
          args:
            - bundle
            - exec
            - sidekiq
            - -c
            - "25"
            - -q
            - default
          env: <%= partial 'rails_environment' %>
          resources: {}
      restartPolicy: Always
